{
    "name": "praips.io",
    "version": "1.0.0",
    "main": "index.js",
    "repository": "https://gitlab.com/BorislavBorisov22/praips.io",
    "author": "BorislavBorisov22 <borislavborisov22@abv.bg>",
    "scripts": {
        "start": "concurrently --kill-others \"npm run webpack\" \"npm run dev\"",
        "dev": "nodemon --exec babel-node lib/server.js",
        "webpack": "webpack -wd",
        "build-webpack": "webpack -p",
        "build-node": "babel lib -d build --copy-files",
        "start-prod": "nodemon build/server.js"
    },
    "babel": {
        "presets": [
            "react",
            [
                "env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ]
        ],
        "plugins": [
            "transform-class-properties",
            "transform-object-rest-spread"
        ]
    },
    "devDependencies": {
        "babel-eslint": "^8.2.3",
        "concurrently": "^3.5.1",
        "eslint": "^4.19.1",
        "eslint-plugin-react": "^7.8.2",
        "redux-immutable-state-invariant": "^2.1.0",
        "webpack-cli": "^2.1.3"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "babel-cli": "^6.26.0",
        "babel-loader": "^7.1.4",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "ejs": "^2.6.1",
        "express.js": "^1.0.0",
        "nodemon": "^1.17.4",
        "prop-types": "^15.6.1",
        "react": "^16.3.2",
        "react-dom": "^16.3.2",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.3.1",
        "redux": "^4.0.0",
        "redux-saga": "^0.16.0",
        "webpack": "3.0.0"
    }
}
